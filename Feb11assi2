sno=int(input("enter sno: "))
sname=input("enter sname:")
s1=int(input("enter s1 marks:"))
s2=int(input("enter s2 marks:"))
s3=int(input("enter s3 marks:"))
Total=s1+s2+s3
avg=Total/3
if (s1 or s2 or s3 )<35:
    print("fail")
elif avg>=91:
    print ("o-grade")
elif avg>=81:
    print ("a-grade")
elif avg>=71:
    print("b-grade")
elif avg>=61:
    print("c-grade")
elif avg>=51:
    print("d-grade")
elif avg>=41:
    print("pass")
else:
    print("fail")
